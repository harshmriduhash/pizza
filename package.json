{
  "name": "hungrybot",
  "version": "2.0.0",
  "description": "",
  "scripts": {
    "babel:server": "babel --presets es2015,stage-0 --ignore node_modules,public ./src/server --out-dir ./compiled/server --source-maps",
    "babel:twitchbot": "babel --presets es2015,stage-0 --ignore node_modules,public ./src/twitchbot --out-dir ./compiled/twitchbot --source-maps --copy-files",
    "babel:lib": "babel --presets es2015,stage-0 --ignore node_modules,public ./src/lib --out-dir ./compiled/lib --source-maps",
    "babel:db": "babel --presets es2015,stage-0 --ignore node_modules,public ./src/db --out-dir ./compiled/db --source-maps",
    "sequelize": "node_modules/.bin/sequelize",
    "sequelize:int": "NODE_ENV=integration node_modules/.bin/sequelize",
    "sequelize:prod": "NODE_ENV=production node_modules/.bin/sequelize",
    "lint": "./node_modules/eslint/bin/eslint.js ./src/server ./src/lib ./src/db ./src/twitchbot",
    "build:all": "npm run lint && npm run babel:lib && npm run babel:db && npm run babel:twitchbot && npm run babel:server",
    "run:dev": "NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js && npm run build:all && NODE_ENV=development node ./compiled/server/app.js",
    "run:debug:dev": "npm run build:all && NODE_ENV=development devtool --no-sm ./compiled/server/app.js",
    "run:twitchbot:dev": "NODE_ENV=development node ./compiled/twitchbot/botserver.js",
    "run:debug:twitchbot:dev": "NODE_ENV=development devtool --no-sm ./compiled/twitchbot/botserver.js",
    "build:int": "NODE_ENV=integration webpack --colors --config ./webpack/webpack.config.prod.js && npm run build:all && cp static/* public/",
    "forever:int": "NODE_ENV=integration forever start -l ../.forever/int_server.log ./compiled/server/app.js",
    "forever:int:twitchbot": "NODE_ENV=integration forever start -l ../.forever/int_twitchbot.log ./compiled/twitchbot/botserver.js",
    "build:prod": "NODE_ENV=production webpack --colors --config ./webpack/webpack.config.prod.js && npm run build:all && cp static/* public/",
    "forever:prod": "NODE_ENV=production forever start -l ../.forever/prod_server.log ./compiled/server/app.js",
    "forever:prod:twitchbot": "NODE_ENV=production forever start -l ../.forever/prod_twitchbot.log ./compiled/twitchbot/botserver.js"
  },
  "repository": "https://github.com/johnhforrest/hungrybot",
  "author": "",
  "license": "MIT",
